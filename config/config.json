    "resolution": {
      "x": 2048,
      "y": 1537
    },
    "fps": 10,
    "capture_mode": true,
    "brightness": 0.5,
    "contrast": 0.5,
    "saturation": 0.5,
    "gain": 0.5,
    "hue": 0.5,
    "auto_focus": true,
